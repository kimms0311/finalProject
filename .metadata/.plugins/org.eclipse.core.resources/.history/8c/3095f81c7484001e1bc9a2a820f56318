<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.avo.www.repository.JobBoardDAO">
	<insert id="post" parameterType="pbvo">
		insert into product(pro_title,pro_email,pro_nick_name,pro_content,pro_price,pro_category)
		values(#{proTitle},#{proEmail},#{proNickName},#{proContent},#{proPrice},'job')
	</insert>
	
	<select id="detail" parameterType="long" resultType="pbvo">
		select * from board where pro_bno = #{proBno}
	</select>



</mapper>